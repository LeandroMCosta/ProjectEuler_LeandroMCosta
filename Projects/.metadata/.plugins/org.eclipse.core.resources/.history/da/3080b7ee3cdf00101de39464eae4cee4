import java.io.IOException;

public class main {

	/**
	 * @param args
	 * @throws IOException 
	 */
	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
		int projeto=-1;
		java.util.Scanner scanner = new java.util.Scanner(System.in);
		while(projeto!=0){
			System.out.println("Escolha o Projeto");
			 projeto = scanner.nextInt();
			switch (projeto){
			case 1: 
				Project1.run();
				break;		
			case 2: 
				Project2.run();
				break;		
			case 3: 
				Project1.run();
				break;		
			case 4: 
				Project1.run();
				break;
			default:
				System.out.println("Projeto inexistente");
			}
		}
	}

}
